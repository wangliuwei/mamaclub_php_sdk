{
  "name": "mamaclub/php-sdk",
  "description": "Mamaclub SDK for PHP",
  "keywords": ["mamaclub", "oauth2","sdk"],
  "type": "library",
  "homepage": "https://github.com/wangliuwei/mamaclub_php_sdk",
  "license": "Facebook Platform",
  "authors": [
    {
      "name": "Mamaclub",
      "homepage": "https://github.com/wangliuwei/mamaclub_php_sdk/graphs/contributors"
    }
  ],
  "require": {
    "php": "^5.4|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "Mamaclub\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mamaclub\\Test\\": "test/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}